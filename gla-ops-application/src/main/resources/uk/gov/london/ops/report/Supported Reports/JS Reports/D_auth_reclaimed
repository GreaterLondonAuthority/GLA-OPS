select distinct   pl.vendor_name     relaimed_Payee,   pl.project_id          reclaimed_project_id,   pl.wbs_code          reclaimed_wbs_code,  pl.sap_vendor_id   reclaimed_sapvendor_id,  pl.category            reclaimed__category,   pl.sub_category     reclaimed_subcategory,  pl.ledger_status    reclaimed_status,  sum(case when pl.ledger_type = 'PAYMENT' THEN (pl.amount*-1) else 0 end)  reclaimed_grant,  sum(case when pl.ledger_type = 'RCGF' THEN (pl.amount*-1) else 0 end)     reclaimed_rcgf,  sum(case when pl.ledger_type = 'DPF' THEN (pl.amount*-1) else 0 end)      reclaimed_dpf,   cast(pl.authorised_on as date) reclaimed_authorised_on ,  pl.authorised_by                     reclaimed_authorised_by,            ''                reclaimed_invoice_filename
from project_ledger_entry pl
where  pl.ledger_status  not in ('Pending', 'Declined')  and sub_category like 'Reclaim%' group by  pl.vendor_name,  pl.project_id, pl.wbs_code, pl.sap_vendor_id, pl.category,  pl.sub_category,  pl.ledger_status, cast(pl.authorised_on as date), pl.authorised_by