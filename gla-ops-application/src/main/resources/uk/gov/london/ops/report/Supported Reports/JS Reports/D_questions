select distinct     pb.project_id,   pb.block_display_name,   pb.block_status question_block_status ,  an.question_id,    q.text question,   an.answer,    an.numeric_answer,  f.id att_id,  f.content_type attachment_content_type, f.file_name    attachment_file_name, f.file_content attachment_file_content, f.created_on   attachment_created_on,  f.created_by   attachment_created_by,  f.organisation_id    from project_block pb   inner join answer an on pb.id = an.questions_block    inner join question q on an.question_id = q.id    left join answer_attachment att on an.id = att.answer_id left join file f on att.attachment_id = f.id   where pb.reporting_version = 'true'