
select distinct     pb.project_id,  pb.block_status gs_block_status,  pb.last_modified  gs_last_modified, gs.zero_grant_requested   gs_zero_grant_requested,  gs.associated_project gs_associated_project,  gs.strategic_funding  gs_strategic_funding,   sum(coalesce(gs.rcgf_value,0))  gs_RCGF,    sum(coalesce(gs.dpf_value,0))   gs_DPF,    sum(coalesce(gs.grant_value,0)) gs_Grant    from project_block pb       inner join grant_source_block gs on pb.id = gs.id     where pb.reporting_version ='true' and pb.block_type = 'GRANT_SOURCE'   group by     pb.project_id ,  gs.zero_grant_requested,  gs.associated_project,  gs.strategic_funding, pb.block_status, pb.last_modified