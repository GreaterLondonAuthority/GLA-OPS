select distinct   pb.project_id,        pb.last_modified  mi_last_modified,     pb.block_status   mi_block_status,   case when cast(substring(to_char(milestone_date,'DD/MM/YYYY'),4,2) as int) in ('1','2','3')  then    text(cast(right( to_char(milestone_date,'DD/MM/YYYY'),4) as int) -1)||'-'||text(cast(right( to_char(milestone_date,'DD/MM/YYYY'),4) as int))     else  text(cast(right(to_char(milestone_date,'DD/MM/YYYY'),4) as int) )||'-'||text(cast(right(to_char(milestone_date,'DD/MM/YYYY'),4) as int)+1)  end mi_milestone_financial_yr   ,   mi.id                 mi_id, mi.milestone_status   mi_milestone_status, mi.milestone_date     mi_milestone_date, mi.monetary_split     mi_monetary_split, mi.summary            mi_milestone, mi.requirement        mi_requirement, mi.display_order      mi_display_order, mi.monetary           mi_monetary, mi.created_on         mi_created_on, mi.manually_created   mi_manually_created, mi.created_by         mi_created_by,  mi.milestones_block   mi_milestones_block, mi.external_id        mi_external_id, mi.conditional        mi_conditional, mi.modified_by        mi_modified_by, mi.modified_on        mi_modified_on, mi.claim_status       mi_claim_status, mi.claimed_grant      mi_claimed_grant, mi.claimed_rcgf       mi_claimed_rcgf, mi.claimed_dpf        mi_claimed_dpf, mi.description        mi_description, mi.monetary_value     mi_monetary_value, mi.key_event          mi_key_event, mi.na_selectable      mi_na_selectable, mi.not_applicable     mi_not_applicable, mi.reclaim_reason     mi_reclaim_reason, mi.reclaimed_grant    mi_reclaimed_grant, mi.reclaimed_dpf      mi_reclaimed_dpf, mi.reclaimed_rcgf     mi_reclaimed_rcgf, mi.reclaimed          mi_reclaimed             from  project_block pb          inner join milestone mi on pb.id = mi.milestones_block     inner join project pj on pb.project_id = pj.id    where pb.reporting_version ='true'