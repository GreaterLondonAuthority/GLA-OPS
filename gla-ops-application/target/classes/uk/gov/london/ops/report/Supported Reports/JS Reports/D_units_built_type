select distinct  pb.project_id       units_built_type_project_id, pb.last_modified   units_built_type_mod, u.new_build_units   units_built_type_new_build_units,	 u.refurbished_units	units_built_type_refurbished_units, u.type_1_units	units_built_type_1_units,		 u.type_2_units	units_built_type_2_units,		 u.type_3_units	units_built_type_3_units,		 u.type_4_units	units_built_type_4_units,		 u.type_5_units	units_built_type_5_units,		 u.type_6_units	units_built_type_6_units,		 u.type_7_units	units_built_type_7_units,		 u.type_8_units	units_built_type_8_units,		 u.nb_wheelchair_units   units_built_type_nb_wheelchair_units,			 u.gross_internal_area    units_built_type_gross_internal_area     from project_block pb inner join unit_details_block u on u.id = pb.id where  pb.block_display_name = 'Unit Details' and pb.reporting_version = 'true'